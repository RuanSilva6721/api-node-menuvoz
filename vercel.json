{
    "version": 2,
    "builds": [
      {
        "src": "src/app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "(.*)",
        "dest": "src/app.js"
      }
    ],
    "headers": [
          {
          "source": "/manifest.webmanifest",
          "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "*" }
          ]
          }
    ]
  }