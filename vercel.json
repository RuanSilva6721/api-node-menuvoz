{
  "version": 2,
  "builds": [
    {
      "src": "src/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "headers": {
        "Access-Control-Allow-Origin": "*"
      },
      "dest": "/src/app.js"
    },
    {
      "src": "(.*)",
      "dest": "/src/app.js"
    }
  ]
}
