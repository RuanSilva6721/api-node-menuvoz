{
  "version": 2,
  "builds": [
    {
      "src": "src/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/manifest.json",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ],
      "dest": "/src/app.js"
    },
    {
      "src": "(.*)",
      "dest": "/src/app.js"
    }
  ]
}
